<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <!--카멜표기법설정-->
    <settings>
        <setting name="mapUnderscoreToCamelCase" value="true" />
    </settings>
    <!-- 패키지로 묶었으므로 domain 객체는 @Alias 어노테이션으로 관리, 만약 domain 모델의 타입이 중복 될 우려가 있을 경우 타입을 각각 관리하도록 한다. -->
    <typeAliases>
        <package name="com.example.dingko" />
    </typeAliases>

</configuration>